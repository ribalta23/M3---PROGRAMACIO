PROGRAMA

    ENTORN

    ALGORITME

        FUNCIÃ“
            
            ENTORN:
                
                N1 = NUMERICA SENCERA
                QUADRAT = NUMERICA REAL
                CUB = NUMERICA REAL
                ARRELQUADRADA = NUMERICA REAL
                ARRELCUBICA = NUMERICA REAL
               

            INICI
                QUADRAT = 0
                CUB = 0
                ARRELQUADRADA = 0
                ARRELCUBICA = 0

                PER N1 DE 1 FINS 15
                    QUADRAT = N1 * N1
                    CUB = N1 * N1 * N1
                    ARRELQUADRADA = arrelquadrada(n1)
                    ARREUCUBICA = arrelcubica(n1)
                FI PER
               
            RETORNA
    
        FI FUNCIO

FI PROGRAMA


        