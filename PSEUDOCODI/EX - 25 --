            ENTORN:
                
               N1 = NUMERICA REAL
               N2 = NUMERICA REAL
               N3 = NUMERICA REAL
               R = NUMERICA REAL
            INICI
                Llegir(N1, N2, N3)
                SI(N1 < 0)
