PROGRAMA

    ENTORN

    ALGORITME

        FUNCIÓ
            
            ENTORN:
                
                N1 = NÚMERICA SENCERA
                N2 = NÚMERICA SENCERA

            INICI
                
                Llegeix(N1 , N2)

                SI(N1 = N2)
                    Escriu(Son iguals)
                SI NO (N1 > N2)
                    Escriure(N1 Es mes gran que N2)
                SI NO
                    Escriure(N2 Es mes gran que N1)
                FI SI
                
            RETORNA
    
        FI FUNCIO

