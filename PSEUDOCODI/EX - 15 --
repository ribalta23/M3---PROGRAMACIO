PROGRAMA

    ENTORN

    ALGORITME

        FUNCIÃ“
            
            ENTORN:
                C = NUMERICA SENCERA
                N = NUMERICA SENCERA
                INSUFICIENT = NUMERICA SENCERA
                SUFICIENT = NUMERICA SENCERA
                BE = NUMERICA SENCERA
                NOTABLE = NUMERICA SENCERA
                EXCELENT = NUMERICA SENCERA
            INICI
                C = 1
                INSUFICIENT = 0
                SUFICIENT = 0
                BE = 0
                NOTABLE = 0
                EXCELENT = 0
                PER C DE 1 FINS 25
                    Escriu(Escriu una nota:)
                    Llegeix(N)
                    SI(N < 5)
                        INSUFICIENT ++
                    SINO SI(N >= 5 I N <= 6)
                        SUFICIENT ++
                    SINO SI(N >= 6 I N < 7)
                        BE ++
                    SINO SI(N >= 7 I N < 9)
                        NOTABLE ++
                    SINO SI(N >= 9 <= 10)
                        EXCELENT ++
                    FI SI
                FI PER
                Escriu(INSUFICIENT)
                Escriu(SUFICIENT)
                Escriu(BE)
                Escriu(NOTABLE)
                Escriu(EXELENT)
            RETORNA
    
        FI FUNCIO

FI PROGRAMA

        